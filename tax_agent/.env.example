# MongoDB
MONGODB_URI=mongodb+srv://...
DATABASE_NAME=georgian_tax_db

# Google AI
GEMINI_API_KEY=your-key
EMBEDDING_MODEL=gemini-embedding-001

# Authentication
API_KEY_SECRET=your-secret

# Tax Agent Settings
SIMILARITY_THRESHOLD=0.5
MATSNE_REQUEST_DELAY=2.0
SEARCH_LIMIT=5
KEYWORD_SEARCH_ENABLED=true
RATE_LIMIT=30
QUERY_REWRITE_MODEL=gemini-2.0-flash
QUERY_REWRITE_TIMEOUT=3.0

# Feature Flags (Orchestrator) â€” all default to false
ROUTER_ENABLED=true
LOGIC_RULES_ENABLED=true
CRITIC_ENABLED=false
CRITIC_CONFIDENCE_THRESHOLD=0.7

# CORS (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:3010

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=false
